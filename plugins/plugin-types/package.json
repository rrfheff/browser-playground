{
  "name": "@browser-playground/plugin-types",
  "version": "0.0.1",
  "description": "TypeScript typings injector for Monaco used by @browser-playground/core.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:rrfheff/browser-playground.git",
    "directory": "plugins/plugin-types"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node scripts/generate-typings.mjs && tsup src/index.ts --format esm,cjs --dts",
    "dev": "node scripts/generate-typings.mjs && tsup src/index.ts --format esm,cjs --dts --watch",
    "lint": "echo 'lint not configured for plugin-types'",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@browser-playground/core": "workspace:*",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/scheduler": "^0.26.0",
    "csstype": "^3.1.3"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.2.0"
  }
}
